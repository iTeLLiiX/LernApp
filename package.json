{
  "name": "flashcard-app",
  "version": "1.0.0",
  "description": "A modern flashcard learning application",
  "type": "module",
  "main": "server/index.ts",
  "scripts": {
    "dev": "vite",
    "dev:db": "node scripts/dev-db.cjs",
    "dev:test": "node scripts/dev-test.cjs",
    "dev:lint": "node scripts/dev-lint.cjs",
    "dev:client": "cd client && npm run dev",
    "dev:server": "node --loader ts-node/esm server/index.ts",
    "build": "tsc && vite build",
    "start": "node scripts/start.cjs",
    "start:server": "node dist/server/index.js",
    "prod": "node scripts/prod.cjs",
    "test": "node scripts/test.cjs",
    "test:server": "jest",
    "test:client": "cd client && npm test",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init --preview-feature",
    "migrate": "node scripts/migrate.cjs",
    "clean": "node scripts/clean.cjs",
    "install:all": "node scripts/install.cjs",
    "db:seed": "node scripts/init-db.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@capacitor/cli": "^7.3.0",
    "@capacitor/core": "^7.3.0",
    "@prisma/client": "^5.0.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.7",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cors": "^2.8.5",
    "dexie": "^3.2.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "lucide-react": "^0.514.0",
    "react": "^18.2.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.11",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.5.0",
    "postcss": "^8.4.35",
    "prisma": "^5.0.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.4.19"
  }
}
