{
  "name": "flashcard-app",
  "version": "1.0.0",
  "description": "A modern flashcard learning application",
  "type": "module",
  "main": "server/index.ts",
  "scripts": {
    "dev": "node scripts/dev.cjs",
    "dev:db": "node scripts/dev-db.cjs",
    "dev:test": "node scripts/dev-test.cjs",
    "dev:lint": "node scripts/dev-lint.cjs",
    "dev:client": "cd client && npm run dev",
    "dev:server": "node --loader ts-node/esm server/index.ts",
    "build": "cd client && npm run build",
    "start": "node scripts/start.cjs",
    "start:server": "node dist/server/index.js",
    "prod": "node scripts/prod.cjs",
    "test": "node scripts/test.cjs",
    "test:server": "jest",
    "test:client": "cd client && npm test",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init --preview-feature",
    "migrate": "node scripts/migrate.cjs",
    "clean": "node scripts/clean.cjs",
    "install:all": "node scripts/install.cjs",
    "db:seed": "node scripts/init-db.js"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "jest": "^29.5.0",
    "prisma": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.3"
  }
}
